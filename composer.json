{
  "name": "Anton-Am/yii2-portfolio-landing",
  "description": "Portfolio landing page powered by yii2 by Anton-Am",
  "keywords": [
    "yii2",
    "framework",
    "landing page",
    "personal site",
    "yii2 advanced"
  ],
  "authors": [
    {
      "name": "Amelin Anton",
      "email": "antonamru@gmail.com",
      "role": "Developer"
    }
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/Anton-Am/yii2-portfolio-landing/issues",
